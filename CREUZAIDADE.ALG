algoritmo "CreuzaIdade"
var
   ano1, ano2, idade: Real
inicio
      escreva (" Creuza, em que ano você nasceu? ")
      leia (ano1)
      
      escreva (" Certo, e em que ano estamos hoje? ")
      leia (ano2)
      
      idade <- (ano2 - ano1)
      
      escreva ("Creuza, você tem", idade , " anos de idade")
fimalgoritmo